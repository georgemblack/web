---
import Head from "../components/Head.astro";

interface Props {
  title?: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <Head title={title} />
  <body>
    <slot />
  </body><script
    is:inline
    async
    src="https://scripts.simpleanalyticscdn.com/latest.js"></script>
</html>
