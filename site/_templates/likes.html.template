{{ define "likes" }}
{{ $root := . }}
<!DOCTYPE html>
<html lang="en">
  {{ template "head" . }}

  <body>
    {{ template "header" . }}
    <main>
      <h1>Likes</h1>
      {{ range .SiteData.Likes.Likes }}
      <a href="{{ .URL }}">{{ .Title }}</a>
      {{ end }}
    </main>
    {{ template "footer" . }}
  </body>
</html>
{{ end }}
