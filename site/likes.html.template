{{ $root := . }}
<!DOCTYPE html>
<html lang="en">
  {{ template "head" . }}
  <body>
    {{ template "header" . }}
    <main>
      <h1>Likes</h1>
      {{ range .SiteData.Likes.Likes }}
      <div class="like">
        <h2><a href="{{ .URL }}">{{ .Title }}</a></h2>
        <p class="subtitle">{{ .URL | getDomainFromURL }}</p>
      </div>
      {{ end }}
    </main>
    {{ template "footer" . }}
  </body>
</html>
